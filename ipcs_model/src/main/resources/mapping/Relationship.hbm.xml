<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ipcs.model.Relationship" table="relationship">
        <id name="objectId" type="java.lang.Long">
            <column name="relationship_objid" />
            <generator class="native" />
        </id>


        <many-to-one name="iswho" column="iswho"
                     class="com.ipcs.model.Person"/>

        <many-to-one name="whose" column="whose"
                     class="com.ipcs.model.Person"/>

        
        <many-to-one name="type" column="relationship_type_fk"
        class="com.ipcs.model.RelationshipType"/>

    </class>
</hibernate-mapping>