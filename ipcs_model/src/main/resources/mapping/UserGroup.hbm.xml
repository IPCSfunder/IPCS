<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ipcs.model.UserGroup" table="user_group">
        <id name="objectId" type="java.lang.Long">
            <column name="user_group_objid" />
            <generator class="native" />
        </id>

        <property name="name" type="string">
            <column name="name" />
        </property>
        
        <property name="createdTime" type="date">
            <column name="date_created"/>
        </property>
        
        <property name="modifiedTime" type="date">
            <column name="date_modified"/> 
        </property>
        
        <many-to-one name="orginizer" column="orginizer"
        class="com.ipcs.model.Person"/>


        <many-to-one name="groupMember" column="group_member"
                     class="com.ipcs.model.Person"/>

    </class>
</hibernate-mapping>